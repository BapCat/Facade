{
  "name": "bapcat/facade",
  "description": "Pseudo-static accessors for Phi bindings",
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/BapCat/Facade",
  "keywords": ["Facade", "IOC", "Inversion of control", "DI", "Dependency injection"],
  "authors": [
    {
      "name": "Corey Frenette",
      "email": "corey@narwhunderful.com"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "platform": {
      "php": "7.4"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "bapcat/tailor": "^0.6",
    "bapcat/propifier": "^3.0",
    "bapcat/phi": "^2.0 | ^3.0 | ^4.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "BapCat\\Facade\\": "src/"
    }
  }
}
